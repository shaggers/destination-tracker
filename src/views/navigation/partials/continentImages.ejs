<% if(continent.name === "Asia") { %>
    <img class="continentImage" src="../asia.jpg" alt="asia">
<% } else if(continent.name === "Africa") { %>
    <img class="continentImage" src="../africa.jpg" alt="africa">
<% } else if(continent.name === "Europe") { %>
    <img class="continentImage" src="../europe.jpg" alt="europe">
<% } else if(continent.name === "North America") { %>
    <img class="continentImage" src="../north_america.jpg" alt="north america">
<% } else if(continent.name === "Oceania") { %>
    <img class="continentImage" src="../australia.jpg" alt="oceania">
<% } else if(continent.name === "South America") { %>
    <img class="continentImage" src="../south_america.jpg" alt="south america">
<% } else if(continent.name === "Antarctica") { %>
    <img class="continentImage" src="../antarctica.jpg" alt="antarctica">
<% } %>